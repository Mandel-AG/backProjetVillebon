<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/postlist.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
    <div class="list-group w-100 d-flex flex-row text-center">

        <div class="list-group w-50 d-flex elements">
            <h3>Liste</h3>
            <% posts.forEach((element) => {%>
            <ul>
                    <li class='list-group-item active'><%= element.id %></li> 
                    <li class='list-group-item'>title :<%= element.title %></li> 
                    <li class='list-group-item'>description : <%= element.description %></li> 
                    <li class='list-group-item'>type :<%= element.type %></li> 
                    <li class='list-group-item'>media :<%= element.media %></li> 
                    <li class='list-group-item'>time :<%= element.createdAt %></li> 
            </ul>
            <% }) %>
        </div>
    </div>
    <script type="text/javascript" src="javascript/list.js" ></script>

</body>
</html>