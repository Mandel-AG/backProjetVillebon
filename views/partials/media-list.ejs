<% medias.forEach((media) => {%>
    <ul>
            <li id='idElement' class='list-group-item active'><%= media.id %></li> 
            <li class='list-group-item'>name :<%= media.name %></li> 
            <li class='list-group-item'>equipe :<%= media.equipe %></li> 
            <li class='list-group-item'>description : <%= media.description %></li> 
            <li class='list-group-item'>chemin :<%= media.chemin %></li> 
            <li class='list-group-item'>time :<%= media.createdAt %></li> 
            <button id='delete' type='button' class="btn btn-danger w-25" mediaid= <%- media._id %> >Delete</button>

    </ul>
    <% }) %>